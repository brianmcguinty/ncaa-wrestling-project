A:link {text-decoration: none; color: black;}
A:visited {text-decoration: none; color: black;}
A:active {text-decoration: none; color: black;}
A:hover {font-size:15; font-weight:bold; color: black;}

#body-color-tag {
	background-color:#EEF8F5;
}

#masthead {
	width: 1000px;
	background-color:rgb(30,144,255);
	text-align:right
	border-radius: 10px;
}

#masthead_table {
	width:100%;
}

#masthead_title {
	text-align:center;
	font-size: 25px;
}

#masthead_username {
	float: right; 
	width: 20%;
	vertical-align:top;
}


#logo_image
{
	float:left;
	height:100px;
	width:75px;
}

#nav {
	background-color:rgb(30,144,255);
	border-radius: 10px;
	float: left; 
	width: 20%;
}

#content {
	float: left; 
	width: 60%;
}

.vertical-button-panel {
	vertical-align:center; 
	border:none; padding:0; 
	background:none; 
	cursor:pointer; 
	*cursor:hand;
}

.aa-picks-panel {
	vertical-align:top; 
}

.content_text{
	text-align:left;
	padding: 10px 15px;
}

#pick_contest {
	margin-left: 10%;
	margin-right: 10%;
}

#new_user_block {
}

#user_login {
	absolute: right; 
	width: 30%;
	vertical-align:bottom;
}

#lsidebar {
	float: left; 
	width: 20%;
	vertical-align:text-top;
}

#left_sidebar_indent {
	text-indent:15px;
}

#rsidebar {
	float: left; 
	width: 20%;
}

#rsidebar_home {
	float: left; 
	width: 20%;
	vertical-align:top;
	horizontal-align:right;
}

#footer {
	width: 100%; 
	clear: both;
	background-color:rgb(30,144,255);
	border-radius: 10px;
}

